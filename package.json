{
  "name": "dlr-buttons",
  "version": "0.0.1",
  "description": "Various React buttons",
  "main": "lib/",
  "scripts": {
    "test": "tape ./spec",
    "pretest": "npm run build",
    "lint": "eslint ./src/*",
    "clean": "rm -rf lib && mkdir lib",
    "build": "babel src --out-dir lib",
    "prebuild": "npm run clean",
    "watch": "babel --watch src --out-dir lib",
    "prepublish": "npm run build",
    "example": "browserify examples/basic.js -d -t babelify > examples/bundle.js"
  },
  "keywords": [
    "React",
    "buttons",
    "interface",
    "UI",
    "DLR"
  ],
  "author": "Matt Styles",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.1.2",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-eslint": "^3.1.20",
    "babelify": "^6.1.2",
    "browserify": "^10.2.4",
    "eslint": "^0.24.0",
    "glob": "^5.0.12",
    "tape": "^4.0.0"
  }
}
